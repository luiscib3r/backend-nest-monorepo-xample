{
  "version": 2,
  "builds": [
    {
      "src": "apps/api-v2/src/main.ts",
      "use": "@vercel/node",
      "outputDirectory": "../../dist/apps/api-v2"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/api-v2/src/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
