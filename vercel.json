{
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  },
  "version": 2,
  "builds": [
    {
      "src": "/apps/api-v2/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "/dist/apps/api-v2/*",
      "use": "@now/dinamic"
    }
  ],
  "routes": [
    {
      "src": "/api2(.*)",
      "dest": "/apps/api-v2/src/main.ts",
      "methods": ["GET"]
    }
  ]
}
